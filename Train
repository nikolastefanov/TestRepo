using System;
using System.Linq;

namespace _01_Train
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            int[] people = new int[n];

            for (int i = 0; i < n; i++)
            {
                people[i] = int.Parse(Console.ReadLine());
            }

            int sum = people.Sum();

            Console.WriteLine(string.Join(" ",people));
            Console.WriteLine(sum);
        }
    }
}
